# open terminal
super + t
	xfce4-terminal
# open rofi drun mode
super + r
	rofi -show drun
# open rofi power menu mode
super + p
	rofi -show power-menu -modi "power-menu:$HOME/.local/bin/rofi-power-menu --choices=lockscreen/logout/reboot/shutdown --confirm=''"
# open flameshot screen capture
Print
	flameshot gui
